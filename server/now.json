{
    "version": 2,
    "name": "travel-log-api",
    "builds": [
      {
        "src": "src/index.js",
        "use": "@now/node-server"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "src/index.js"
      }
    ],
    "alias": [
      "travel-log-api"
    ],
    "env": {
      "NODE_ENV": "production",
      "PORT": "1337",
      "DATABASE_URL": "@travel-log-db",
      "CORS_ORIGIN": "https://travel-log.now.sh",
      "API_KEY": "@travel-log-api-key"
    }
  }
  